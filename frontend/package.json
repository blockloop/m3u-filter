    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "3.1.0",
    "react-scripts": "5.0.1",
    "sass": "1.69.5",
    "source-map-explorer": "2.5.3",
    "typescript": "5.2.2",
    "wait-on": "7.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
